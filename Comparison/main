
load('iJO1366.mat')
load('iJO1366_Rxn_lethals.mat')

sol_minNorm0=optimizeCbModel(model,'max','zero')
sol_minNorm1=optimizeCbModel(model,'max','one')
sol_minNorm2=optimizeCbModel(model,'max',2)

sol_gurobi=optimizeCbModel(model)
changeCobraSolver('glpk','lp')
sol_glpk=optimizeCbModel(model)
changeCobraSolver('cplex_direct','lp')
sol_cplex=optimizeCbModel(model)

classification(1).class=classifyLethals(iJO1366,Jdl,sol_minNorm0.x)
classification(2).class=classifyLethals(iJO1366,Jdl,sol_minNorm1.x)
classification(3).class=classifyLethals(iJO1366,Jdl,sol_minNorm2.x)
classification(4).class=classifyLethals(iJO1366,Jdl,sol_gurobi.x)
classification(5).class=classifyLethals(iJO1366,Jdl,sol_glpk.x)
classification(6).class=classifyLethals(iJO1366,Jdl,sol_cplex.x)